<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <menuitem id="hospital_menu_root"
        name="Hospital"
        sequence="0"/>
                
    <record id="hospital_menu_master" model="ir.ui.menu">
        <field name="name">Patient Details</field>
        <field name="parent_id" ref="hospital_menu_root"/>
        <field name="sequence">10</field>
    </record>

    <record id="menu_configuration" model="ir.ui.menu">
        <field name="name">Configurations</field>
        <field name="parent_id" ref="hospital_menu_root"/>
        <field name="sequence">100</field>
    </record>

    <record id="menu_appointment" model="ir.ui.menu">
        <field name="name">Appointment</field>
        <field name="parent_id" ref="hospital_menu_root"/>
        <field name="sequence">90</field>
    </record>


</odoo>